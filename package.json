{
  "name": "discord-notify-action",
  "version": "1.0.1",
  "private": false,
  "description": "GitHub Action that send a message to a Discord channel.",
  "keywords": [
    "discord",
    "github",
    "action"
  ],
  "homepage": "https://github.com/cabrera-evil/discord-notify-action/#readme",
  "bugs": {
    "url": "https://github.com/cabrera-evil/discord-notify-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cabrera-evil/discord-notify-action"
  },
  "license": "MIT",
  "author": {
    "name": "cabrera-evil",
    "email": "douglascabrera.dev@gmail.com"
  },
  "maintainers": [
    {
      "name": "cabrera-evil",
      "email": "douglascabrera.dev@gmail.com"
    }
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "prepare": "husky",
    "start": "node dist/index",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "pnpm lint:check",
      "pnpm format:check"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "axios": "0.21.1",
    "joi": "^17.13.3",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.0",
    "dotenv": "8.2.0",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=6.0.0"
  }
}
